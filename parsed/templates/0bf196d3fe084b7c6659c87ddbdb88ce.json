{"components/common/wishlist-dropdown":"<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\r\n    <a aria-controls=\"wishlist-dropdown\" aria-expanded=\"false\" class=\"button dropdown-menu-button\" data-dropdown=\"wishlist-dropdown\">\r\n        <span>Add To Wishlist</span>\r\n        <i aria-hidden=\"true\" class=\"icon\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-chevron-down\" />\r\n            </svg>\r\n        </i>\r\n    </a>\r\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown\" tabindex=\"-1\">\r\n        {{#if customer.wishlists}} {{#each customer.wishlists}}\r\n        <li>\r\n            <input class=\"button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\" formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\r\n                type=\"submit\" value=\"{{this.name}}\">\r\n        </li>\r\n        {{/each}} {{else}}\r\n        <li>\r\n            <input class=\"button\" type=\"submit\" value=\"Add To wishlist\">\r\n        </li>\r\n        {{/if}}\r\n        <li>\r\n            <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">Create New List</a>\r\n        </li>\r\n    </ul>\r\n</form>\r\n"}
