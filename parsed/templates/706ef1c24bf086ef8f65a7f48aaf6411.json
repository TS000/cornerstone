{"components/common/quick-search":"<div class=\"top-search\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">Search</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"on\">\n                <button class=\"timmy search-icon\" type=\"submit\">\n                <i class=\"fa fa-search\" aria-hidden=\"true\"></i>\n                </button>\n            </div>\n        </fieldset>\n    </form>\n    <!-- <section class=\"quickSearchResults\" data-bind=\"html: results\"></section> -->\n</div>\n"}
