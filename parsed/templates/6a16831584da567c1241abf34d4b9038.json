{"components/carousel-content":"<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    {{#if theme_settings.carouselBackground }}\n    {{/if}}\n    <div class=\"carousel-content-wrap\"> \n\n    <h1 class=\"heroCarousel-title\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description\">{{{text}}}</p>\n        \n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"/search.php\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <div class=\"input-wrap\">\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"on\" onClick=\"return document.getElementById('search_query').value !=''\"/>\n                <button class=\"timmy search-icon\" type=\"submit\"><i class=\"fa fa-search\" aria-hidden=\"true\" onClick=\"return document.getElementById('search_query').value !=''\"></i></button></div>\n                \n            </div>\n        </div>\n        </fieldset>\n    </form>\n</div>\n</div>\n"}
